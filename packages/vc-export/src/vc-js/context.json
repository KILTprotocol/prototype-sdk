{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "cred": "https://www.w3.org/2018/credentials#",
    "sec": "https://w3id.org/security#",
    "sec2": "https://w3id.org/security/v2#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "kiltCred": "https://www.kilt.io/contexts/credentials#",
    "KiltCredential2020": {
      "@id": "kiltCred:KiltCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "delegationId": {
          "@id": "kiltCred:delegationId",
          "@type": "@id"
        },
        "legitimationIds": {
          "@id": "kiltCred:legitimationIds",
          "@type": "VerifiableCredential"
        },
        "nonTransferable": {
          "@id": "cred:nonTransferable",
          "@type": "xsd:boolean"
        }
      }
    },
    "verificationMethod": "sec:verificationMethod",
    "publicKeyHex": "sec:publicKeyHex",
    "proofPurpose":"sec:proofPurpose",
    "KILTSelfSigned2020": {
      "@id": "kiltCred:KILTSelfSigned2020",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "signature": "sec:proofValue"
      }
    },
    "Ed25519VerificationKey2018": "sec2:Ed25519VerificationKey2018",
    "KILTAttestation2020": {
      "@id": "kiltCred:KILTAttestation2020",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "attesterAddress": "cred:issuer"
      }
    },
    "KILTCredentialDigest2020": {
      "@id": "kiltCred:KILTCredentialDigest2020",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "claimHashes": {
          "@id": "kiltCred:KILTCredentialDigest2020#claimHashes",
          "@type": "sec:digestValue"
        },
        "nonces": {
          "@id": "kiltCred:KILTCredentialDigest2020#nonces",
          "@type": "sec:nonce",
          "@container": "@index"
        }
      }
    }
  }
}